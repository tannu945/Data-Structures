class linked:
    def __init__(self, data = None):
        self.data = data
        self.next = None
class headvalue:
    def __init__(self):
        self.val = None

    def insert_at_begin(self, object1, val):
        newnode=linked(val)
        newnode.next = self.val
        self.val = newnode

    def traverse(self):
        i = self.val
        while i is not None:
            print(i.data)
            i=i.next
list1 = headvalue()
list1.val = linked("Mon")
e2 = linked("Tue")
e3 = linked("Wed")
list1.val.next = e2
e2.next = e3
list1.insert_at_begin(list1.val, "Sun")
list1.traverse()
