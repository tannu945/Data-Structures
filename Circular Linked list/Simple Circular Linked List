class clinked:
    def __init__(self, data = None):
        self.data = data
        self.next = None
class headval:
    def __int__(self):
        self.val = None
        
list = headval()
list.val = clinked(1)
e1 = clinked(2)
e2 = clinked(3)
e3 = clinked(4)
list.val.next = e1
e1.next= e2
e2.next = e3
e3.next = list.val

